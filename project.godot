config_version=5

[application]

config/name="Dice Simulator"
config/description="This is my first experiment with 3D and physics in Godot. It's dice. Just dice. Throw them."
run/main_scene="res://node_3d.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
run/max_fps=30
config/icon="res://icon.svg"

[filesystem]

import/blender/enabled.android=true
import/blender/enabled.web=true

[physics]

3d/run_on_separate_thread=true

[rendering]

textures/lossless_compression/force_png=true
